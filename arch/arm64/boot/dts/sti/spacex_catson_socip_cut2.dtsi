	l3netmmap: l3reg_mmap {
		status = "disabled";
		compatible = "sx,simple-mmap";
		#address-cells = <1>;
		#size-cells = <1>;
		ranges = <0x0c209000 0 0x0c209000 0x1000>;

		l3_net {
			reg = <0x0c209000 0x1000>;
		};
	};

	/* L3 UL PUSH + L3 DL PULL */
	catson_pushpull_l3gmac: pp_l3gmac@0c20c000 {
		status = "disabled";
		compatible = "sx,apb-dma-push-pull-net-2.00a";
		suffix = "l3_gmac";

		reg =  <0 0x0c20c000 0x1000
			0 0x0c20b000 0x1000 >;

		interrupt-parent = <&gic>;
		interrupts = < 0 183 4
			       0 180 4 >;

		desc-type = "l3gmac_net";

		push {
			fifo-depth = <1024>;

			/* Power of 2 roundup of maximum ethernet frame (1522) */
			buffer-size = <2048>;
		};

		pull {
			fifo-depth = <1024>;

			/* Power of 2 roundup of maximum ethernet frame (1522) */
			buffer-size = <2048>;
		};
	}; /* L3 GMAC */

	/* L3 DL PUSH + L3 UL PULL */
	catson_pushpull_uplink: catson_pushpull_uplink {
		status = "disabled";
		compatible = "sx,apb-dma-push-pull-2.00a";
		suffix = "cl3ul";

		reg = < 0 0x0c20d000 0x1000
			0 0x0c20a000 0x1000 >;

		interrupt-parent = <&gic>;
		interrupts = < 0 181 4
			       0 182 4 >;

		desc-type = "cl3ul";

		push {
			fifo-depth = <1024>;

			/* Large enough for SpaceX header + IP packet */
			buffer-size = <2048>;
		};

		pull {
			fifo-depth = <1024>;

			/* Large enough for SpaceX header + IP packet */
			buffer-size = <2048>;
		};
	}; /* L3 Uplink Pushpull */

