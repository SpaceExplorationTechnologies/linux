	};

	upstream_modemlink_monitor: modemlink_monitor@00c208004 {
		status = "disabled";
		compatible = "sx,catson-mdml-monitor-1.00";

		phys = <&miphys2 PHY_TYPE_MDML>;
		phy-names = "miphy";

